#!/usr/bin/env mkb
#Builder mkb file for the java portion of the s3eFacebook extension on android

platform JAVA

files
{
    (source/android)
    s3eFacebook.java
	com/facebook/android/AsyncFacebookRunner.java
	com/facebook/android/DialogError.java
	com/facebook/android/Facebook.java
	com/facebook/android/FacebookError.java
	com/facebook/android/FbDialog.java
	com/facebook/android/Util.java
}

librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/s3eFacebook.jar
